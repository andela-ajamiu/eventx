<% unless @sponsor.errors.any? %>
$("#eventSponsors").html("<%= j(render('event_sponsors', sponsors: @sponsors))%>")
$("#addSponsorModal").closeModal();
$("#toast").html("<%= j(render('event_sponsor_toast')) %>");
<% else %>
$("#toast").html("<%= j(render('event_sponsor_toast')) %>");
$("#addSponsorModal").closeModal();
<% end %>
