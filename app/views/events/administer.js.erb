$(document).ready(function () {
  $('#manage-event-btn').click(function (event) {
    $('#content').html('<%= j(render template: "events/edit", layout: "admin" %>');
    event.preventDefault()
  });

  $('#manage-staff-btn').click(function (event) {
    $('#content').html('<%= j(render file: "manager_profiles/manage_staff" %>');
    event.preventDefault()
  });

  $('#manage-task-btn').click(function (event) {
    $('#content').html('<%= j(render file: "taska/show" %>');
    event.preventDefault()
  });

  $('#manage-sponsor-btn').click(function (event) {
    $('#content').html('<%= j(render file: "sponsors/show %>');
    event.preventDefault()
  });
});
